{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended",
        "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        // "indent": [
        //     "error",
        //     4
        // ],
        // "linebreak-style": [
        //     "error",
        //     "windows"
        // ],
        "quotes": [
            "error",
            "double"
        ],
        // "keyword-spacing": [
        //     "error",
        //     {
        //         "before": true,
        //         "after": true
        //     }
        // ],
        // "space-before-function-paren": [
        //     "error",
        //     {
        //         "anonymous": "always",
        //         "named": "never",
        //         "asyncArrow": "always"
        //     }
        // ],
        "eqeqeq": [
            "error",
            "always"
        ],
        // "space-infix-ops": [
        //     "error"
        // ],
        // "comma-spacing": [
        //     "error"
        // ],
        // "brace-style": [
        //     "error",
        //     "1tbs",
        //     {
        //         "allowSingleLine": false
        //     }
        // ],
        "curly": [
            "error",
            "all"
        ],
        "handle-callback-err": [
            "error",
            "err"
        ],
        // "no-multiple-empty-lines": [
        //     "error",
        //     {
        //         "max": 1
        //     }
        // ],
        // "operator-linebreak": [
        //     "error",
        //     "after",
        //     {
        //         "overrides": {
        //             "?": "after",
        //             ":": "after"
        //         }
        //     }
        // ],
        "one-var": [
            "error",
            "never"
        ],
        // "block-spacing": [
        //     "error",
        //     "always"
        // ],
        // "comma-dangle": [
        //     "error",
        //     "never"
        // ],
        // "comma-style": [
        //     "error"
        // ],
        // "dot-location": [
        //     "error",
        //     "object"
        // ],
        // "eol-last": [
        //     "error",
        //     "always"
        // ],
        // "func-call-spacing": [
        //     "error",
        //     "never"
        // ],
        // "key-spacing": [
        //     "error"
        // ],
        // "new-parens": [
        //     "error"
        // ],
        "accessor-pairs": [
            "error"
        ],
        "no-array-constructor": [
            "error"
        ],
        "no-caller": [
            "error"
        ],
        "no-duplicate-imports": [
            "error"
        ],
        "no-eval": [
            "error"
        ],
        "no-extend-native": [
            "error"
        ],
        "no-extra-bind": [
            "error"
        ],
        "no-extra-boolean-cast": [
            "error"
        ],
        // "no-extra-parens": [
        //     "error"
        // ],
        // "no-floating-decimal": [
        //     "error"
        // ],
        "no-global-assign": [
            "error",
            {
                "exceptions": [
                    "Object"
                ]
            }
        ],
        "no-implied-eval": [
            "error"
        ],
        "no-iterator": [
            "error"
        ],
        "no-label-var": [
            "error"
        ],
        "no-labels": [
            "error"
        ],
        "no-lone-blocks": [
            "error"
        ],
        // "no-multi-spaces": [
        //     "error"
        // ],
        "no-multi-str": [
            "error"
        ],
        "no-new": [
            "error"
        ],
        "no-new-func": [
            "error"
        ],
        "no-new-object": [
            "error"
        ],
        "no-new-require": [
            "error"
        ],
        "no-new-wrappers": [
            "error"
        ],
        "no-obj-calls": [
            "error"
        ],
        "no-octal-escape": [
            "error"
        ],
        "no-proto": [
            "error"
        ],
        "no-return-assign": [
            "error"
        ],
        "no-self-compare": [
            "error"
        ],
        "no-sequences": [
            "error"
        ],
        "no-tabs": [
            "error",
            {
                "allowIndentationTabs": true
            }
        ],
        "no-throw-literal": [
            "error"
        ],
        // "no-trailing-spaces": [
        //     "error"
        // ],
        "no-undef-init": [
            "error"
        ],
        "no-unneeded-ternary": [
            "error"
        ],
        "no-useless-call": [
            "error"
        ],
        "no-useless-computed-key": [
            "error"
        ],
        "no-useless-rename": [
            "error"
        ],
        // "no-whitespace-before-property": [
        //     "error"
        // ],
        // "object-property-newline": [
        //     "error",
        //     {
        //         "allowAllPropertiesOnSameLine": true
        //     }
        // ],
        // "padded-blocks": [
        //     "error",
        //     "never"
        // ],
        // "rest-spread-spacing": [
        //     "error"
        // ],
        // "semi-spacing": [
        //     "error",
        //     {
        //         "before": false,
        //         "after": true
        //     }
        // ],
        // "space-before-blocks": [
        //     "error",
        //     "always"
        // ],
        // "space-in-parens": [
        //     "error"
        // ],
        // "space-unary-ops": [
        //     "error"
        // ],
        "spaced-comment": [
            "error",
            "always"
        ],
        // "template-curly-spacing": [
        //     "error"
        // ],
        // "wrap-iife": [
        //     "error",
        //     "inside"
        // ],
        // "yield-star-spacing": [
        //     "error"
        // ],
        "yoda": [
            "error"
        ],
        "@typescript-eslint/no-empty-interface": [
            "error",
            {
                "allowSingleExtends": true
            }
        ],
        // "object-curly-spacing": [
        //     "error"
        // ],
        "semi": [
            "off",
            "always"
        ],
        // "arrow-spacing": [
        //     "error"
        // ],
        "no-console": [
            "warn"
        ]
    }
}